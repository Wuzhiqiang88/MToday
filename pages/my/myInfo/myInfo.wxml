<!--pages/my/my.wxml-->
<view class='page'>
  <view class="weui-cells__title"></view>
  <view class="weui-cells weui-cells_after-title">
    <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__hd">
        <image src="/icon/tx.svg"></image>
      </view>
      <view class="weui-cell__bd">头像</view>
      <view class="weui-cell__ft weui-cell__ft_in-access">
        <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
          <image class="weui-media-box__thumb" src="{{userInfo.avatarUrl}}" />
        </view>
      </view>
    </navigator>
  </view>
  <view class="weui-cells__title"></view>
  <view class="weui-cells weui-cells_after-title">
    <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__hd">
        <image src="/icon/user.svg"></image>
      </view>
      <view class="weui-cell__bd">名字</view>
      <view class="weui-cell__ft weui-cell__ft_in-access">{{userInfo.nickName}}</view>
    </navigator>
    <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__hd">
        <image src="/icon/gender.svg"></image>
      </view>
      <view class="weui-cell__bd">性别</view>
      <view class="weui-cell__ft weui-cell__ft_in-access">{{gender[userInfo.gender]}}</view>
    </navigator>
  </view>
  <view class="weui-cells__title"></view>
  <view class="weui-cells weui-cells_after-title">
    <navigator url= "/pages/my/changeBirthday/changeBirthday" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__hd">
        <image src="/icon/birthday.svg"></image>
      </view>
      <view class="weui-cell__bd">出生日期</view>
      <view class="weui-cell__ft weui-cell__ft_in-access">{{birthday}}</view>
    </navigator>
  </view>
</view>